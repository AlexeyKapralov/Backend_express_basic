{
	"arrowParens": "avoid",
	"useTabs": true,
	"singleQuote": true,
	"tabWidth": 2,
	"semi": false,
	"trailingComma": "none",
	"importOrder": [
		"<THIRD_PARTY_MODULES",
		"^@/src/(.*)$",
		"^@/src/db/(.*)$",
		"^@/components/(.*)$",
		"^@/components/layout/(.*)$",
		"^@/components/screens/(.*)$",
		"^@/components/ui/(.*)$",
		"^@/styles/(.*)$",
		"^@/config/(.*)$",
		"^@/utils/(.*)$",
		"^@/services/(.*)$",
		"^@/store/(.*)$",
		"^@/api/(.*)$",
		"^.+\\.(template\\.html|module\\.scss)$",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"bracketSpacing": true,
	"arrayBracketSpacing": true
}
